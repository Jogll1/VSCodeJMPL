{
    "name": "vscode-jmpl",
    "publisher": "JMPL",
    "displayName": "JMPL",
    "description": "A VS Code plugin for the JMPL language.",
    "version": "0.0.2",
    "engines": {
            "vscode": "^1.96.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jmpl",
            "aliases": ["JMPL", "jmpl"],
            "extensions": [".jmpl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jmpl",
            "scopeName": "source.jmpl",
            "path": "./syntaxes/jmpl.tmLanguage.json"
        }]
    }
}
