{
    "name": "vscode-jmpl",
    "publisher": "Jogll1",
    "displayName": "JMPL",
    "icon": "images/JMPLMascot.png",
    "repository": "https://github.com/Jogll1/VSCodeJMPL",
    "description": "A VS Code plugin for the JMPL language.",
    "version": "0.2.0",
    "engines": {
        "vscode": "^1.96.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "jmpl",
            "aliases": ["JMPL", "jmpl"],
            "extensions": [".jmpl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "jmpl",
            "scopeName": "source.jmpl",
            "path": "./syntaxes/jmpl.tmLanguage.json"
        }],
        "snippets": [{
            "language": "jmpl",
            "path": "./snippets/jmpl.code-snippets.json"
        }]
    }
}
